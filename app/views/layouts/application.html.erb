<!DOCTYPE html>
<html ng-app="mallesti">
<head>
  <title>Mallesti</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
 
  <table ng-controller="CustomersController as customersCtrl">
  	<caption>TABLA DE DAMIAN</caption>
  	<thead>
  		<tr>
  			<th>NOMBRE</th>
  			<th>EMAIL</th>
  			<th>TLF</th>
  		</tr>
  	</thead>
  	<tbody>
  		<tr ng-repeat="customer in customersCtrl.customers">
  			<td>{{customer.name}}</td>
  			<td>{{customer.email}}</td>
  			<td>{{customer.phone}}</td>
  		</tr>
  		
  	</tbody>
  </table>

</body>
</html>
